{% include "custom_block_style" %}

<!-- Block Variables -->
{% assign b-Code   = block.settings.code %}

<!-- KJB Settings Variables -->
{% assign kjb-Code = 'code' | settings_id: section: section, block: block %}

<!-- Force Line Break -->
<div class="block-break--{{ b-ID }}"></div>

<div class="{{ b-Width }} block--{{ b-ID }}">
  <div kjb-settings-id="{{ kjb-Code }}" class="fluid-video">
    {{ b-Code }}
  </div>
</div>

<!-- Force Line Break -->
<div class="block-break--{{ b-ID }}"></div>
